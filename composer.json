    "type": "yii2-extension",
    "minimum-stability": "stable",
    "keywords": ["yii2","extension","usuario","ldap"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Marco Piazza",
            "email": "marco.piazza@yetopen.it"
        },
        {
            "name": "Liviu Calin",
            "email": "liviu.calin@yetopen.it"
        },
        {
            "name": "Elias Luhr",
            "email": "e.luhr@herzogkommunikation.de"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "2amigos/yii2-usuario": "~1.0",
        "adldap2/adldap2": "~9.1.2"
    },
    "autoload": {
        "psr-4": {
            "yetopen\\usuarioLdap\\": ""
        }
    }
